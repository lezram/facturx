{
  "name": "@lezram/facturx",
  "version": "0.2.0",
  "description": "Factur-X and Order-X generation library for European e-invoicing standard",
  "repository": "stafyniaksacha/facturx",
  "keywords": [
    "factur-x",
    "order-x",
    "e-invoicing",
    "pdf",
    "xml",
    "european",
    "en16931"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "schema",
    "bin"
  ],
  "bin": {
    "facturx": "./bin/facturx.mjs"
  },
  "scripts": {
    "prepack": "unbuild",
    "dev": "unbuild --watch",
    "build": "unbuild",
    "test": "vitest"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "date-fns": "^3.6.0",
    "libxmljs2": "github:lezram/libxmljs2",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "typescript": "^5.5.4",
    "unbuild": "3.0.0-rc.7",
    "vitest": "^2.0.5"
  }
}
